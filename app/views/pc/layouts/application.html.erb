<!DOCTYPE html>
<html>
  <head>
    <title>MealApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'pc/application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'pc/css/jTinder.css', 'data-turbolinks-track': 'reload' %>

    <%= javascript_include_tag 'pc/application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'pc/js/jquery.min.js', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'pc/js/jquery.transform2d.js', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'pc/js/jquery.jTinder.js', 'data-turbolinks-track' => true %>
  </head>
  <script>$("#tinderslide").jTinder();</script>

  <body>
    <%= yield %>
  </body>
</html>
