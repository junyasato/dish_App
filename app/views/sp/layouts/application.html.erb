<!DOCTYPE html>
<html>
  <head>
    <title>MealApp</title>
    <%= csrf_meta_tags %>
    <meta name="viewport" content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no">

    <%= stylesheet_link_tag    'sp/application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <link rel="stylesheet" type="text/css" href="<%= asset_path 'sp/css/jTinder.css' %>">

    <%= javascript_include_tag 'sp/application', 'data-turbolinks-track': 'reload' %>

    <script type="text/javascript" src="<%= asset_path 'sp/js/jquery.min.js' %>"></script>
    <script type="text/javascript" src="<%= asset_path 'sp/js/jquery.transform2d.js' %>"></script>
    <script type="text/javascript" src="<%= asset_path 'sp/js/jquery.jTinder.js' %>"></script>

  </head>
  <script>$("#tinderslide").jTinder();</script>

  <body>
    <%= yield %>
  </body>
</html>
